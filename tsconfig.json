{
  "compileOnSave": true,
  "compilerOptions": {
      "outDir": "./dist",
      "sourceMap": true,
      "noEmitOnError": true,
      "strict": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noErrorTruncation": true,
      "noFallthroughCasesInSwitch": true,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react"
  },
  "include": [
    "./src/**/*"
  ]
}
